{
    "configurations": [
        {
            "name": "Python Debugger: Python File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}"
        },
        {
            "name": "Run: structs.structs",
            "type": "debugpy",
            "request": "launch",
            "module": "app.structs.structs"
        },
        {
            "name": "Run: unpack.text_unpacker",
            "type": "debugpy",
            "request": "launch",
            "module": "app.unpack.text_unpacker"
        },
        {
            "name": "Run: editor.save_editor",
            "type": "debugpy",
            "request": "launch",
            "module": "app.editor.save_editor"
        },
        {
            "name": "Entry (Debug Mode)",
            "type": "debugpy",
            "request": "launch",
            "module": "app.entry",
            "args": [ "--debug" ]
        },
    ]
}